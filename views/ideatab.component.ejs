<% var firstNode; %>
<div class="show__idea__list">
  <div class="show__idea__header">
    <h3>My Ideas</h3>
    <img
      src="../images/remove.png"
      width="22"
      alt=""
      onclick="onClickRemoveBtnIdea()"
    />
  </div>
  <div class="show__idea__content">
    <% fetchData.map((data,key) => { %> <% console.log(data) %> <%
    console.log("data") %> <% if(data.userId === userId){ %> <% if(key <
    firstNode || firstNode === undefined){%> <% firstNode = key; %> <% } %> <%
    if(key === parseInt(noteId)){ %>
    <div class="content__idea__list" id="<%= key %>idea">
      <div
        class="info"
        onclick='getIdeaData("<%= key %>","<%= data.idea_data %>","<%= firstNode %>","<%=data.id %>","<%=noteId %>")'
      >
        <h3><%=data.idea_title %></h3>
        <div class="idea__time">
          <span id="spanTimeUpdate">4:52 PM</span>
        </div>
      </div>
      <div
        id="removeContainerIdea"
        class="removeClass"
        onclick="removeIdeaFromList('<%=data.id%>')"
      >
        <img
          width="20"
          src="data:image/svg+xml;base64,"
        />
      </div>
    </div>
    <% }else{ %>
    <div class="content__idea__list list__notFocus" id="<%= key %>idea">
      <div
        class="info"
        onclick='getIdeaData("<%= key %>","<%= data.idea_data %>","<%= firstNode %>","<%=data.id %>","<%=noteId %>")'
      >
        <h3><%=data.idea_title %></h3>
        <div class="idea__time">
          <span id="spanTimeUpdate">4:52 PM</span>
        </div>
      </div>
      <div
        id="removeContainerIdea"
        class="removeClass"
        onclick="removeIdeaFromList('<%=data.id%>')"
      >
        <img
          width="20"
          src="data:image/svg+xml;base64,"
        />
      </div>
    </div>
    <% } %> <% } %> <% }) %>
  </div>
  <div class="add__idea">
    <a href="javascript:void(0)" onClick="showAddIdea('show');">Add new Idea</a>
  </div>
</div>

<% if(fetchData[firstNode] !== undefined) { %>
<div class="editor__container" id="edit_container">
  <textarea
    id="textAreaContent"
    name="editor"
    cols="30"
    rows="10"
    class="ckeditor"
  >
<%=fetchData[noteId].idea_data%></textarea
  >
  <script type="text/javascript">
    runFetchCkEditor("<%=fetchData[noteId].id%>");
  </script>
  <!-- <div class="editor__container__button">
                <button type="button" onclick="showContent()">Save</button>
            </div> -->
</div>
<% }else{%>
<div class="no__fetchData">
  <h1>No Data</h1>
</div>
<% }%>

<div class="add__note__settings" id="showAddIdea">
  <div class="add__note__options">
    <div class="exit__btn">
      <button type="button" onClick="showAddIdea('hide');">X</button>
    </div>
    <div class="add__note__content">
      <label for="noteTitle">Idea Title</label>
      <div class="text__container">
        <input type="text" id="ideaTitle" /><br />
        <button type="button" onClick="addIdea('<%=fetchData.length%>')">
          Save
        </button>
      </div>
    </div>
  </div>
</div>
